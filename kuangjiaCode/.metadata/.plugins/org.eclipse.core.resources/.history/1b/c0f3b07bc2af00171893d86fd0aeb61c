<mapper namespace="com.atguigu.mybatis.dao.EmployeeMapperDynamicSQL">

	<select id="getEmpsByConditonIf" resultType="com.atguigu.mybaits.beans.Employee">
		select * from tal_employee 
		where id = #{id}
		and last_name = #{lastName}	
		and email = #{email}
		and gender = #{gender}
	</select>


</mapper>