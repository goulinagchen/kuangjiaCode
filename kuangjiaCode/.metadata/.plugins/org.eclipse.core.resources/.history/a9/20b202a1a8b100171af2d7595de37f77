<mapper namespace="ssm.mapper.DepartmentMapper">
	
	<select id="getDepartments" resultType="ssm.bean.Department">
		select *
		from tal_dept
	</select>
	<select id="getDepartment" resultType="ssm.bean.Department">
		select *
		from tal_dept
		where id = #{id}
	</select>
	
</mapper>